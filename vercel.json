{
    "version": 2,
    "builds": [
      {
        "src": "frontend/src/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "backend/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/src/$1"
      }
    ]
  }